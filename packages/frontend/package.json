{
  "name": "sam-react-node-boilerplate-frontend",
  "version": "0.0.1",
  "author": "Craig Barratt",
  "description": "SAM React Node Boilerplate - Frontend",
  "license": "MIT",
  "scripts": {
    "start": "PORT=5555 react-scripts start",
    "build": "NODE_ENV=production react-scripts test --coverage --watchAll=false && react-scripts build",
    "test": "react-scripts test",
    "test:ci": "yarn test --coverage --watchAll=false",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "yarn lint --fix",
    "generate:component": "npx generate-react-cli component"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux-persist": "^6.0.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-proxy-middleware": "^1.0.6",
    "prettier": "2.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
